  transform: rotate(270deg);
  transition: visibility 0.4s linear 0.4s, opacity 0.4s linear 0.3s;
  width: 140px;
}
.plyr--full-ui .plyr__volume:before {
  border-left: 4px solid transparent;
  border-right: 4px solid transparent;
